#include<bits/stdc++.h>
#include<stdio.h>
using namespace std;

int fibo(int i) {

   if(i == 0) {
      return 0;
   }

   if(i == 1) {
      return 1;
   }
   return fibo(i-1) + fibo(i-2);
}

int  main() {

   int i,n;
   printf("Enter a Number:");
   scanf("%d",&n);

   for (i = 0; i < n; i++)
{
      printf("%d\t\n", fibo(i));
   }

   return 0;
}
